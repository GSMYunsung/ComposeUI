<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kdn.studycompose_ui">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudyCompose_UI">
        <activity
            android:name=".ProfileCardLayout"
            android:exported="true"
            android:label="@string/title_activity_profile_card_layout"
            android:theme="@style/Theme.StudyCompose_UI" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DynamicContentExample"
            android:exported="true"
            android:label="@string/title_activity_dynamic_content_example"
            android:theme="@style/Theme.StudyCompose_UI">
        </activity>
        <activity
            android:name=".ComposeActivity"
            android:exported="false"
            android:label="@string/title_activity_compose"
            android:theme="@style/Theme.StudyCompose_UI" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.StudyCompose_UI.NoActionBar" />
    </application>

</manifest>